import SwiftUI

struct AlunoView: View {
    init() {
            UITabBar.appearance().backgroundColor = UIColor.azulR
        }
    var body: some View {
        TabView{
            HomeAlunoView(user: Aluno(nome: "", matricula: "", faltas: 1, idDisc: [1]))
                .tabItem {
                    Image(systemName: "house.fill")
                }
            ListaAlunoView()
                .tabItem {
                    Image(systemName: "line.3.horizontal")
                }
        }
        .frame(maxHeight: .infinity)
        .accentColor(.white)
    }
    
    
    
}

#Preview {
    AlunoView()
}
